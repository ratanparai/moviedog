            		url: "http://172.27.27.84/fetch_movie_screen_shots",
            		data: { 'imdb_id': $value },
            		success: function(data) {
            			var screen_shots="";
                		var i;
						for (i = 0; i < data.length; i++) { 
  							screen_shot=document.getElementById("screen_shot_template").innerHTML;
  							var screen_shot = screen_shot.replace("screen_shot_url_link",data[i].file_path);
  							var screen_shot = screen_shot.replace("screen_shot_url",data[i].file_path);
  							var div = document.getElementById('screen_shot_list');
  							div.innerHTML += screen_shot;  						
						}                	
            		}
        		});
    		}	
		}
		
		
		function fetch_movie_casts(imdb_id){

			$value=imdb_id;
			list=document.getElementById("cast_list").innerHTML;
			if(imdb_id!="N/A" && list=="<h2></h2>"){
				document.getElementById("cast_list").innerHTML="";
			///////////////////////////////////////////////////////
				$.ajax({
            		type: 'get',
            		url: "http://172.27.27.84/fetch_movie_casts",
            		data: { 'imdb_id': $value },
            		success: function(data) {
            			for (variable in data) {
            				var cast=document.getElementById("cast_template").innerHTML;
  							cast = cast.replace("cast_url_link",data[variable].profile_path);
  							cast = cast.replace("cast_url",data[variable].profile_path);
  							cast = cast.replace("actor_name_link",data[variable].name);
  							cast = cast.replace("actor_name",data[variable].name);

  							var div = document.getElementById('cast_list');
  							console.log(cast);
  							div.innerHTML += cast;  	  
  						
						};
            		}
        		});
        	}	
		}

