<script>
    var search_on = 0;
    function search() {
        if(search_on == 0) {
            $('#search_result').css("display", "block");
            search_on = 1;
        }
        else {
            $('#search_result').css("display", "none");
            search_on = 0;
        }
    }
    function searchResult(value) {

        var check_string = value.split(' ').join('');

        if(check_string.length!=0) {
            console.log(value);
            $.ajax({
                type: 'GET',
                url: '/ajax_search',
                data: {'search_value': value},
                success: function (data) {
                    $('.header__search-content').css('height', '370px');
                    $('.header__search').css('height', '370px');
                    console.log(data);
                    document.getElementById('search_result_list').innerHTML = data;
                },
                error: function (xhr, status, error) {
                    var err = eval("(" + xhr.responseText + ")");
                    console.log(error);
                }
            })
        }else {
            document.getElementById('search_result_list').innerHTML = "";
            $('.header__search-content').css('height', '60px');
            $('.header__search').css('height', '60px');


        }
    }
</script>