	<script type="text/javascript">
var matched1 =0;
var matched2 =0;
  function insertData() {
	event.preventDefault();
    var Rcate=$("#Rcate").val();
    var Rname=$("#Rname").val();
    var Rmobile=$("#Rmobile").val();
	var movieName=$("#movieName").val();
	var movieYear=$("#movieYear").val();
    var Rtext=$("#Rtext").val();

	if(Rcate == ''){

		alertify.alert("Input Problem","Please Input Category");
	}
	else if(Rname == ''){

		alertify.alert("Input Problem","Please Input User ID");

	}
	else if(Rmobile == ''){

		alertify.alert("Input Problem","Please Input mobile");

	}
	else if(movieName == ''){

		alertify.alert("Input Problem","Please Input File Name");

	}
	else if((matched1==0)||(matched2==0)){
		alertify.alert("Input Problem","User ID and Mobile Number Doesnot Match");

	}

	else{
		// AJAX code to send data to php file.
        $.ajax({
            type: "POST",
            url: "insert-data.php",
            data: {Rcate:Rcate,Rname:Rname,Rmobile:Rmobile,movieName:movieName,Rtext:Rtext,movieYear:movieYear},

            success: function(data) {
            $("#message").html(data);
            $("#message").addClass("alert alert-success");
			alertify.alert("Success", "Your Upload Request is successful, We will notify you after uploading this content by SMS.");
					  window.setTimeout(function(){location.reload()},1000)
            },
            error: function(err) {
            alert(err);
            }
        });

	}



}



function userIdCheck() {
        var userId = $("#Rname").val();
        if (userId.length >= 9) {





          $.ajax({
            type: "POST",
            url: "includes/userIdCheck.php",
            data: { userId: userId},

            success: function(data) {
				if (data != 0) {

                alertify.alert("Wrong Input", data);
				matched1 =0;
              }else{
				matched1 =1;
			  }
            },
            error: function(err) {
            alert(err);
            }
        });


        }
      }


	  function userMobileCheck() {
        var userId = $("#Rname").val();
        var userMobile = $("#Rmobile").val();

        if (userId == "") {
          if (userMobile.length >= 1) {
            alertify.alert("Wrong Input", "Please Input User Id First");
          }
        } else {
          if (userMobile.length >= 11) {

            $.ajax({

				type: "POST",
              url: "includes/userMobileCheck.php",
              data: {
                userId: userId,
                userMobile: userMobile
              },

              success: function(data) {
                if (data != 0) {
                  alertify.alert("Wrong Input", data);

				  matched2 = 0;
                }
				else{

				  matched2 = 1;

				}

              }
            });
          }

        }

      }

  </script>

